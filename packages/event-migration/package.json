{
  "name": "event-migration",
  "version": "1.0.0",
  "private": true,
  "description": "PagoPA Interoperability event migration",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:autofix": "eslint . --ext .ts,.tsx --fix",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src",
    "start:migrate": "node --no-warnings --loader ts-node/esm -r 'dotenv-flow/config' ./src/index.ts",
    "start:verify": "node --no-warnings --loader ts-node/esm -r 'dotenv-flow/config' ./src/read-models-migration-check.ts",
    "build": "tsc"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/node": "^20.3.1",
    "connection-string": "^4.3.6",
    "dotenv-flow": "^3.2.0",
    "json-diff": "^1.0.6",
    "lodash.isequal": "^4.5.0",
    "mongodb": "5.6.0",
    "pagopa-interop-commons": "workspace:*",
    "pagopa-interop-commons-test": "workspace:*",
    "pagopa-interop-models": "workspace:*",
    "pg-promise": "^11.5.0",
    "ts-pattern": "^5.0.6",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/json-diff": "^1.0.3",
    "@types/lodash.isequal": "^4.5.8",
    "eslint": "^8.44.0",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "tsc-esm-fix": "^2.20.15",
    "typescript": "^5.1.3",
    "vitest": "^0.33.0"
  }
}
