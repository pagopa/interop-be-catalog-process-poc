{
  "name": "pagopa-interop-tenant-readmodel-writer",
  "private": true,
  "version": "1.0.0",
  "description": "PagoPA Interoperability tenant consumer service that updates the read model when events are stored",
  "main": "dist",
  "type": "module",
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx",
    "lint:autofix": "eslint . --ext .ts,.tsx --fix",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src",
    "start": "node --watch --loader ts-node/esm -r 'dotenv-flow/config' ./src/index.ts",
    "build": "tsc",
    "check": "tsc --project tsconfig.check.json"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@pagopa/eslint-config": "3.0.0",
    "@types/dotenv-flow": "3.3.3",
    "@types/node": "20.12.12",
    "prettier": "2.8.8",
    "ts-node": "10.9.2",
    "typescript": "5.4.5"
  },
  "dependencies": {
    "@protobuf-ts/runtime": "2.9.4",
    "connection-string": "4.4.0",
    "dotenv-flow": "3.3.0",
    "kafkajs": "2.2.4",
    "pagopa-interop-commons": "workspace:*",
    "pagopa-interop-models": "workspace:*",
    "kafka-iam-auth": "workspace:*",
    "ts-pattern": "5.1.1",
    "zod": "3.23.8"
  }
}
