meta {
  name: Get attribute by name
  type: http
  seq: 2
}

get {
  url: {{host-attribute}}/attributes/name/:name
  body: json
  auth: none
}

params:path {
  name: myattr
}

headers {
  Content-Type: application/json
  Accept: */*
  Accept-Encoding: gzip, deflate, br
  Authorization: {{JWT}}
  X-Correlation-Id: {{correlation-id}}
  Content-Type: application/json
  ~Connection: keep-alive
}
